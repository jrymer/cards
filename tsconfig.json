{
  "compilerOptions": {
      "esModuleInterop": true,
      "outDir": "./dist/",
      "sourceMap": true,
      "noEmit": true,
      "noImplicitAny": true,
      "module": "commonjs",
      "lib": ["es2019"],
      "target": "es6",
      "jsx": "react",
      "baseUrl": ".",
      "paths": {
        "components/*": ["src/components/*"],
        "store/*": ["src/redux/*"],
        "models/*": ["src/models/*"],
        "utils/*": ["src/utils/*"]
      }
  },
  "exclude": ["node_modules", "dist"]
}